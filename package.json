{
  "name": "pnpmkambrium",
  "private": "true",
  "version": "1.0.0",
  "description": "pnpmkambrium is a [opinionated,convention-over-configuration] monorepo toolchain based on pnpm and make",
  "homepage": "https://lgersman.github.io/pnpmkambrium/",
  "engines": {
    "node": ">= 18.8.0",
    "pnpm": ">= 7.14.0"
  },
  "scripts": {
    "prepare": "pnpm dlx only-allow pnpm && (make --silent init || echo \"failed to initialize @pnpmkambrium/core : please run 'make --silent -f node_modules/@pnpmkambrium/core/make/make.mk init  manually'\")"
  },
  "keywords": [
    "pnpm",
    "make",
    "monorepo",
    "npm",
    "docker"
  ],
  "author": {
    "email": "lars.gersmann@cm4all.com",
    "name": "lars gersmann"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgersman/pnpmkambrium"
  },
  "license": "MIT-2",
  "devDependencies": {
    "@pnpmkambrium/core": "workspace:*",
    "@changesets/cli": "2.25.2",
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "git-cz": "4.9.0",
    "nano-staged": "0.8.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "prettier": "2.7.1"
  }
}
