#!/usr/bin/env bash

script_dir=$(cd "$(dirname "${BASH_SOURCE[0]}")" &>/dev/null && pwd -P)

cat <<EOF
[
  {
    "caption" : "Mount",
    "help" : "$script_dir/mount.md.sh",
    "exec" : "echo 'mounted\nPress any key to continue ...'; read  -n 1; . $SHAUNCH_COMMAND"
  },
  {
    "caption" : "Status",
    "help" : "$script_dir/status.md",
    "exec" : ". $SHAUNCH_COMMAND"
  },
  {
    "caption" : "Exit",
    "help" : "# Exit\n\nQuits the program",
    "exec" : "echo 'Good bye'"
  }
]
EOF
